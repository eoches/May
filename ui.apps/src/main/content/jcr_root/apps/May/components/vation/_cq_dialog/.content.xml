<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="creating cq:dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="articletitle"
                name="./text"/>
            <num
                jcr:primaryType="nt:unstructured"
                sling:resourceType="[granite/ui/components/foundation/form/numberfield]"
                fieldLabel="[articleNumber]"
                name="[./num]"/>
            <des
                jcr:primaryType="nt:unstructured"
                sling:resourceType="[granite/ui/components/coral/foundation/form/textarea]"
                fieldLabel="[articledes]"
                name="[./des]"/>
            <multifield
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                composite="{Boolean}true">
                <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container"
                    name="./bookdetailswithmap">
                    <items jcr:primaryType="nt:unstructured">
                        <bookname
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="BookName"
                            name="./bookname"/>
                        <bookdes
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                            fieldLabel="Bookdes"
                            name="./bookdes"/>
                        <bookpublishdate
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                            fieldLabel="Date"
                            name="./bookpublishdate"/>
                        <booknum
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                            fieldLabel="Booknumber"
                            name="./booknum"/>
                        <multifield
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            composite="{Boolean}true">
                            <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                name="./bookdetailswithwriter">
                                <items jcr:primaryType="nt:unstructured">
                                    <writername
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="WriterName"
                                        name="./writername"/>
                                    <writerinfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="WriterInfo"
                                        name="./writerinfo"/>
                                    <writerdateofbirth
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="WriterDateofBirth"
                                        name="./writerdob"/>
                                    <writernum
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldLabel="Writernumber"
                                        name="./writernum"/>
                                </items>
                            </field>
                        </multifield>
                    </items>
                </field>
            </multifield>
            <checkbox
                jcr:primaryType="nt:unstructured"
                sling:resourceType="[granite/ui/components/foundation/form/checkbox]"
                fieldLabel="[Checkbox]"
                name="[./checkbox]"/>
        </items>
    </content>
</jcr:root>